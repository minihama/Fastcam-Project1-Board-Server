<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.mybatis.mybatis_test.TmtMapper">
    <select id="list" resultType="com.mybatis.mybatis_test.TmtVO">
        select * from tmt
    </select>
    <select id="tmtSelect" parameterType="int" resultType="com.mybatis.mybatis_test.TmtVO">
        select * from tmt where id = #{id}
    </select>
</mapper>